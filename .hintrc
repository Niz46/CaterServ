{
  "extends": [
    "development"
  ],
  "hints": {
    "axe/name-role-value": [
      "default",
      {
        "button-name": "off",
        "link-name": "off"
      }
    ],
    "no-inline-styles": "off",
    "axe/parsing": [
      "default",
      {
        "duplicate-id-aria": "off"
      }
    ],
    "compat-api/html": [
      "default",
      {
        "ignore": [
          "iframe[allow]"
        ]
      }
    ]
  },
  "browserslist": [
    "defaults",
    "not ie 11",
    "not and_chr <= 115",
    "not chrome <= 116",
    "not edge <= 115",
    "not firefox <= 116",
    "not safari <= 16.5",
    "not samsung <= 21"
  ]
}